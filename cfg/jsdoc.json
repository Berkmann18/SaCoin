{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_|.+\\.min.js",
    "exclude": [ "(.*)node_modules" ],
    "include": [ "src" ]
  },
  "plugins": ["plugins/summarize"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    }
  },
  "opts": {
    "destination": "./doc/",
    "recurse": true,
    "package": "./package.json",
    "access": "public",
    "readme": "./README.md"
  }
}
